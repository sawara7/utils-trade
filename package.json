{
  "name": "utils-trade",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "rmDist": "node -e 'fs.rmSync(`dist`, {recursive:true, force:true})'",
    "rmNodeModules": "node -e 'fs.rmSync(`node_modules`, {recursive:true, force:true})'",
    "rmPackageLock": "node -e 'fs.rmSync(`package-lock.json`, {recursive:true, force:true})'",
    "cleanup": "npm run rmDist && npm run rmNodeModules && npm run rmPackageLock",
    "build": "npm run cleanup && npm install && tsc",
    "tsc": "tsc",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "preset": "ts-jest/presets/default-esm"
  },
  "author": "sawara",
  "license": "MIT",
  "bundleDependencies": [],
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/uuid": "^9.0.5",
    "typescript": "^5.5.3",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "moment-timezone": "^0.5.41",
    "utils-general": "github:sawara7/utils-general"
  }
}
